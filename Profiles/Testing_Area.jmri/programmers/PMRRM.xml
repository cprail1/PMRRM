<?xml version="1.0" encoding="utf-8"?>
<!-- Draft custom programmer format for the PMRRM                       -->
<programmer-config xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/programmer.xsd">
  <!-- made from Advanced version 19 -->
  <version version="1" author="Bob Jacobsen" lastUpdated="20260120"/>
  <programmer decoderFilePanes="yes" showEmptyPanes="no" showFnLanelPane="yes" showRosterMediaPane="no">

    <pane>
      <name>PMRRM Controls</name>
      <column>
        <dccaddress/>
        <display item="Analog (DC) Operation"/>
        <separator/>
        <display item="Speed Table Definition" format="radiobuttons"/>
        <separator/>
        <display item="Kick Start"/>
        <display item="Accel"/>
        <display item="Decel"/>
      </column>
      <column>
        <row>
          <column>
            <display item="Vstart" layout="below" format="vslider" label=""/>
            <display item="Vstart" layout="below"/>
          </column>
          <column>
            <display item="Vmid" layout="below" format="vslider" label=""/>
            <display item="Vmid" layout="below"/>
          </column>
          <column>
            <display item="Vhigh" layout="below" format="vslider" label=""/>
            <display item="Vhigh" layout="below"/>
          </column>
        </row>
        <display item="Forward Trim"/>
        <display item="Reverse Trim"/>
      </column>
    </pane>

    <!-- all panes fron Advanced programmer -->
    <xi:include href="file:xml/programmers/parts/BasicPane.xml"/>
    <xi:include href="file:xml/programmers/parts/MotorPane.xml"/>
    <xi:include href="file:xml/programmers/parts/BasicSpeedControlPane.xml"/>
    <xi:include href="file:xml/programmers/parts/SpeedTablePane.xml"/>
    <xi:include href="file:xml/programmers/parts/FunctionMapPane.xml"/>
    <xi:include href="file:xml/programmers/parts/LightsPane.xml"/>
    <xi:include href="file:xml/programmers/parts/AnalogControlsPane.xml"/>
    <xi:include href="file:xml/programmers/parts/ConsistPane.xml"/>
    <xi:include href="file:xml/programmers/parts/AdvancedPane.xml"/>
    <xi:include href="file:xml/programmers/parts/SoundPane.xml"/>
    <xi:include href="file:xml/programmers/parts/SoundLevelsPane.xml"/>
    <xi:include href="file:xml/programmers/parts/CVsPane.xml"/>

  </programmer>
</programmer-config>
