<?xml version="1.0" encoding="UTF-8"?>
<auxiliary-configuration xmlns="http://www.netbeans.org/ns/auxiliary-configuration/1">
    <connections xmlns="http://jmri.org/xml/schema/auxiliary-configuration/connections-2-9-6.xsd">
        <connection xmlns="" class="jmri.jmrix.can.adapters.gridconnect.lccbuffer.serialdriver.configurexml.ConnectionConfigXml" disabled="no" manufacturer="LCC" port="COM4" reconnectMaxAttempts="100" reconnectMaxInterval="120" speed="57600" systemPrefix="M" turnoutInterval="100" userName="LCC">
            <options>
                <option>
                    <name>Protocol</name>
                    <value>OpenLCB</value>
                </option>
            </options>
            <node name="Ident">
                <parameter name="UserName">JMRI PMRRM Direct profile</parameter>
                <parameter name="NodeId"/>
                <parameter name="UserDescription"/>
            </node>
            <node name="FastClock">
                <parameter name="EnableMode">disabled</parameter>
                <parameter name="ClockCustomId"/>
                <parameter name="ClockId">default</parameter>
            </node>
        </connection>
        <connection xmlns="" class="jmri.jmrix.loconet.usb_dcs240.configurexml.ConnectionConfigXml" disabled="no" manufacturer="Digitrax" port="COM5" reconnectMaxAttempts="100" reconnectMaxInterval="120" speed="57600" systemPrefix="L" turnoutInterval="250" userName="LocoNet">
            <options>
                <option>
                    <name>CommandStation</name>
                    <value>DCS240 (Advanced Command Station)</value>
                </option>
                <option>
                    <name>FlowControl</name>
                    <value>hardware flow control (recommended)</value>
                </option>
                <option>
                    <name>InterrogateOnStart</name>
                    <value>Yes</value>
                </option>
                <option>
                    <name>LoconetProtocolAutoDetect</name>
                    <value>Auto Detect</value>
                </option>
                <option>
                    <name>PacketizerType</name>
                    <value>Normal (recommended)</value>
                </option>
                <option>
                    <name>TranspondingPresent</name>
                    <value>No</value>
                </option>
                <option>
                    <name>TurnoutHandle</name>
                    <value>Spread</value>
                </option>
            </options>
        </connection>
    </connections>
    <startup xmlns="http://jmri.org/xml/schema/auxiliary-configuration/startup-4-3-5.xsd">
        <perform xmlns="" class="jmri.util.startup.configurexml.PerformScriptModelXml" enabled="yes" name="preference:ChangeDefaultBackupFileName.py" type="ScriptFile"/>
    </startup>
</auxiliary-configuration>
